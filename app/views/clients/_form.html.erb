<script type="text/javascript">
  $(function() {
    $('.date_picker').datetimepicker({
    });
  });
</script>

<%= simple_form_for(@client) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :division %>
    <%= f.input :ticketing_provider %>
    <%= f.input :region %>
    <%= f.input :launch_date, :as => :date_picker %>
    <%= f.input :billing_start_date, :as => :date_picker %>
    <%= f.input :internal %>
    <%= f.association :price %>
    <%= f.input :finance_contact_name %>
    <%= f.input :finance_contact_email %>
    <%= f.input :finance_contact_phone %>
    <%= f.input :venue_contact_name %>
    <%= f.input :venue_contact_email %>
    <%= f.input :venue_contact_phone %>
    <%= f.input :notes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

